<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<VBox alignment="CENTER_LEFT" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.lab04.HelloController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>

  </padding>

  <Label fx:id="Username"/>
  <Label fx:id="Password"/>
  <Label fx:id="Name"/>
  <Label fx:id="Phone"/>
  <Label fx:id="Email"/>
  <Label fx:id="Birthday"/>

  <TextField fx:id="tfUsername"/>
  <TextField fx:id="tfPassword"/>
  <TextField fx:id="tfName"/>
  <TextField fx:id="tfPhone"/>
  <TextField fx:id="tfEmail"/>
  <TextField fx:id="tfBirthday"/>
  <Button text="Register" onAction="#onHelloButtonClick"/>
</VBox>
